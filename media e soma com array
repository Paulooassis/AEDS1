#include <stdio.h>

int main(){
    int num[5], soma = 0;

    for(int i=0; i< 5; i++){
        printf("Digite o %d numero: ", i+1);
        scanf("%d", &num[i]);
        soma += num[i];
    }
    float media;
    media = (float)soma / 5;
    printf("A soma e %d\n", soma);
    printf("A media e %.2f", media);
}
